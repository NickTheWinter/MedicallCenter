<Page x:Class="MedicallCenter.Pages.PrintPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MedicallCenter.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="100" d:DesignWidth="300"
      Title="PrintPage">

    <Grid Background="White">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="17*" />
            <ColumnDefinition Width="58*" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="93*"/>
            <RowDefinition Height="307*"/>
        </Grid.RowDefinitions>
        <TextBlock x:Name="TitleService" Text="Title Service" Grid.Column="1" VerticalAlignment="Center" Height="16"/>
        <TextBlock x:Name="IdService" Text="99" HorizontalAlignment="Center" VerticalAlignment="Center" Height="16" Width="14"  />
        <TextBlock x:Name="PriceService" Text="999$" Grid.Row="1" HorizontalAlignment="Center" VerticalAlignment="Center" Height="16" Width="26"  />
        <Image x:Name="ImageService" Grid.Column="1" Source="/Resources/Image/profile.png" HorizontalAlignment="Left" Margin="0,23,0,0" Grid.RowSpan="2" Width="77"/>
    </Grid>
</Page>
